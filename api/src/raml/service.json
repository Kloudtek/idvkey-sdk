{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "suggestedSecurityLevel": {
      "type": "string",
      "enum": [
        "LOW",
        "STANDARD",
        "HIGH",
        "VERY_HIGH"
      ]
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    }
  }
}